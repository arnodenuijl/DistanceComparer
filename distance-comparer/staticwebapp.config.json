{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,ts,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/robots.txt"
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
